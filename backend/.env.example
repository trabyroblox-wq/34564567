# backend/.env.example
# copy to .env and fill in

PORT=3000
NODE_ENV=development

# optional – if you want to restrict which domains can use the proxy
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080,https://yourdomain.com

